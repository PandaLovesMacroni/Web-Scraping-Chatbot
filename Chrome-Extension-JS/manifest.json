{
    "name" : "Spider Scrapper",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": 
        "spider.png"
    
  },
  "content_scripts":[
    {
        "matches":["<all_urls>"],
        "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker" :"background.js"
  }
}